<h1>Listing focusing_sections</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Industry</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @focusing_sections.each do |focusing_section| %>
  <tr>
    <td><%= focusing_section.name %></td>
    <td><%= focusing_section.industry_id %></td>
    <td><%= link_to 'Show', focusing_section %></td>
    <td><%= link_to 'Edit', edit_focusing_section_path(focusing_section) %></td>
    <td><%= link_to 'Destroy', focusing_section, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

